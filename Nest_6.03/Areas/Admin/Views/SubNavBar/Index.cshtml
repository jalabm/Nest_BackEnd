@model SubNavBar


<form enctype="multipart/form-data" method="post">
    <div class="form-group">
        <label asp-for="Name">Name</label>
        <input class="form-control" asp-for="Name" placeholder="Enter name">
        <span asp-validation-for="Name" class="text-danger"></span>
    </div>
    <div class="form-group">
        <label asp-for="NavbarId">Navbar</label>
        <input class="form-control" asp-for="NavbarId" placeholder="Enter Navbar">
        <span asp-validation-for="NavbarId" class="text-danger"></span>
    </div>
    <div asp-validation-summary="ModelOnly" class="text-danger">  </div>
    <button type="submit" class="btn btn-primary">Save</button>
</form>

